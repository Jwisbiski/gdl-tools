			>
			["gt0020"] = (RULE) <
				when = <"$gt0006!=1|local::at0020|Present|", "$gt0008==local::at0055|Present|", "$gt0014!=null">
				then = <"$gt0006=1|local::at0020|Present|",...>
				priority = <3>
			>
			["gt0021"] = (RULE) <
				then = <"$gt0003.magnitude=((($gt0004.value+$gt0005.value)+$gt0006.value)+$gt0007.value)",...>
				priority = <2>
			>
			["gt0022"] = (RULE) <
				when = <"$gt0023==null",...>
				then = <"$gt0023.magnitude=$gt0014.count",...>
				priority = <1>
			>
		>
