		rules = <
			["gt0015"] = (RULE) <
				when = <"$gt0002==null", "$gt0007==null", "$gt0006==null", "$gt0005==null", "$gt0004==null">
				then = <"$gt0002=0|local::at0019|Absent|", "$gt0007=0|local::at0019|Absent|", "$gt0006=0|local::at0019|Absent|", "$gt0005=0|local::at0019|Absent|", "$gt0004=0|local::at0019|Absent|">
				priority = <8>
			>
			["gt0016"] = (RULE) <
				when = <"$gt0002!=1|local::at0020|Present|", "(($gt0012>=6,mmol/l)&&($gt0013.value<=($currentDateTime.value-18,a)))||(($gt0012>=4,mmol/l)&&($gt0013.value>($currentDateTime.value-18,a)))">
				then = <"$gt0002=1|local::at0020|Present|",...>
				priority = <7>
			>
			["gt0017"] = (RULE) <
				when = <"$gt0004!=1|local::at0020|Present|", "$gt0011==local::at0055|Present|", "$gt0014!=null">
